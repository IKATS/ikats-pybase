# This environment file is used by docker-compose.yml
# to configure the images to use when building a test environment


# Ikats App

# git repo ikats-hmi
IMAGE_HMI=hub.ops.ikats.org/ikats-hmi:latest

# git repo ikats-base
IMAGE_DATAMODEL=hub.ops.ikats.org/ikats-datamodel:latest


# Ikats tools
IMAGE_PYBASE=hub.ops.ikats.org/ikats-pybase:latest
IMAGE_TOMCAT=hub.ops.ikats.org/tomcat:latest
IMAGE_POSTGRES=postgres:9.5
IMAGE_HBASE=dajobe/hbase:latest
IMAGE_OPENTSDB=hub.ops.ikats.org/opentsdb:latest
IMAGE_SPARK=hub.ops.ikats.org/ikats-spark

# Docker bindings
DOCKER_BINDINGS_POSTGRES=/var/lib/ikats/docker_bindings/postgresql
DOCKER_BINDINGS_HBASE=/var/lib/ikats/docker_bindings/hbase/hbase
